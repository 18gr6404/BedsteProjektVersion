<AnchorPane prefHeight="800.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.161" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ch.controller.CreateAsthmaAppUserCtrl">
   <children>
      <Label layoutX="204.0" layoutY="158.0" text="Patienten er ikke oprettet som bruger af nogen app">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <GridPane layoutX="356.0" layoutY="318.0" prefHeight="117.0" prefWidth="461.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="319.0" minWidth="10.0" prefWidth="319.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="146.0" minWidth="0.0" prefWidth="66.5" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="59.0" minHeight="10.0" prefHeight="58.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="59.0" minHeight="10.0" prefHeight="59.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="App:" />
            <Label text="Indhent data registreret i appen før dags dato:" GridPane.rowIndex="1" />
            <ChoiceBox fx:id="chosenAppDropdown" prefWidth="150.0" GridPane.columnIndex="1" />
            <CheckBox fx:id="pastDataWantedCheckbox" mnemonicParsing="false" GridPane.columnIndex="1" GridPane.rowIndex="1" />
         </children>
      </GridPane>
      <Button fx:id="okButton" layoutX="570.0" layoutY="481.0" mnemonicParsing="false" onAction="#handleOk" text="Bekræft" />
      <Label layoutX="539.0" layoutY="268.0" text="Opret her:">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
